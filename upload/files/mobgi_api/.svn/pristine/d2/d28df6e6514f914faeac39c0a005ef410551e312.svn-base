<div class="colMain">
    <div class="title">
        <p class="fl">push推送信息详情</p>
    </div>
    <div class="main">
        <div class="content">
            <div class="app-detail" style="">
                <form method="POST" action="/rtb/save_blacklist" style="">
                    <ul class="form" style="">
                        <!-- loop pushLogArr -->
                        <li>
                            <span class="label">{{pushLogArr' key}}：</span>
                            <div class="fc">
                                
                                <!-- if {{pushLogArr' key}}=='extra' --> 
                                    <textarea style="height:250px;width:80%" value="" readonly>{{pushLogArr' value}}</textarea>
                                <!-- else -->
                                    <input type="text" value="{{pushLogArr' value}}" required  class="iText" style="width:100px;" readonly/>
                                <!-- endif -->
                                
                                
                            </div>
                        </li>
                        <!-- endloop -->
                        <li>
                            <span class="label">JSON数据视图：</span>
                            <div class="fc">
                                <a href="http://www.bejson.com/go.html?u=http://www.bejson.com/jsonview2/" target="_blank">http://www.bejson.com/go.html?u=http://www.bejson.com/jsonview2/</a>
                            </div>
                        </li>
                        <li>
                            <div class="fc">
                                <input type="button" onclick="javascript:history.go(-1);" class="cancel" value="返回">
                            </div>
                        </li>
                        
                    </ul>
                </form>
            </div>
        </div>
    </div>
</div>