<script type="text/javascript" src="{{rootUrl}}/js/jquery.cookie.js"></script>
<script>
$(function(){
    $("#menu").find(".title").css({cursor:'pointer'}).live('click', function(){
        var dis = $(this).next('ul');
        if (dis.css('display') == 'none') {
            dis.show();
        }else{
            dis.hide();
        }
    });
    $("#menu").find("li").click(function(){
        var clsid = $(this).attr("id");
        $.cookie("clsid", clsid, { expires: 7, path: '/' });
    }).removeClass("cur");
    var clsId = $.cookie("clsid");
    var url = window.location.href;
    if (url == "{{siteUrl}}apps/index") {
        $.removeCookie("clsid");
        $("#mli_3").addClass('cur');
    }else{
        $("#"+clsId).addClass('cur');
    }
});
</script>
