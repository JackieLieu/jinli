<!DOCTYPE html>
<html>
<head>
<meta content="text/html;charset=utf-8" http-equiv="Content-Type">
<title>{{project_title}}</title>
<link rel="shortcut icon" href="{{rootUrl}}/favicon.ico" />
<link media="screen" href="{{rootUrl}}/css/reset.css" rel="stylesheet" type="text/css">
<link media="screen" href="{{rootUrl}}/css/layout.css" rel="stylesheet" type="text/css">
<link media="screen" href="{{rootUrl}}/css/jquery-ui-1.8.22.custom.css" rel="stylesheet" type="text/css">
<script src="{{rootUrl}}/js/jquery-1.8.2.min.js" type="text/javascript"></script>
<script src="{{rootUrl}}/js/jquery-ui-1.8.22.custom.min.js" type="text/javascript"></script>
<script type="text/javascript" src="{{rootUrl}}/js/highcharts.js"></script>
<script type="text/javascript" src="{{rootUrl}}/js/hightcharts/exporting.js"></script>
</head>
<body>
<div class="colMain">
    <div class="main">
        <div class="grid">
            <a class="fr btn" href="{{siteUrl}}Statis/getOneByAdId?aid={{aid}}&pid={{pid}}&field=impressions&sdate={{sdate}}&edate={{edate}}&type=day">日期</a>
            <a class="fr btn" href="{{siteUrl}}Statis/getOneByAdId?aid={{aid}}&pid={{pid}}&field=impressions&sdate={{sdate}}&edate={{edate}}&type=hour">小时</a>
            <table>
                <thead><tr>
                        <th>日期</th>
                        <th><a href="{{siteUrl}}Statis/getOneByAdId?aid={{aid}}&pid={{pid}}&field=impressions&sdate={{sdate}}&edate={{edate}}&type={{type}}">展示数</a></th>
                        <th><a href="{{siteUrl}}Statis/getOneByAdId?aid={{aid}}&pid={{pid}}&field=impressions_rate&sdate={{sdate}}&edate={{edate}}&type={{type}}">展示比例</a></th>
                        <th><a href="{{siteUrl}}Statis/getOneByAdId?aid={{aid}}&pid={{pid}}&field=clicks&sdate={{sdate}}&edate={{edate}}&type={{type}}">点击数</a></th>
                        <th><a href="{{siteUrl}}Statis/getOneByAdId?aid={{aid}}&pid={{pid}}&field=clicks_rate&sdate={{sdate}}&edate={{edate}}&type={{type}}">点击率</a></th>
                        <th><a href="{{siteUrl}}Statis/getOneByAdId?aid={{aid}}&pid={{pid}}&field=finish_downloads&sdate={{sdate}}&edate={{edate}}&type={{type}}">下载完成数</a></th>
                        <th><a href="{{siteUrl}}Statis/getOneByAdId?aid={{aid}}&pid={{pid}}&field=installed&sdate={{sdate}}&edate={{edate}}&type={{type}}">安装数</a></th>
                        <th><a href="{{siteUrl}}Statis/getOneByAdId?aid={{aid}}&pid={{pid}}&field=startups&sdate={{sdate}}&edate={{edate}}&type={{type}}">启动数</a></th>
                        <th><a href="{{siteUrl}}Statis/getOneByAdId?aid={{aid}}&pid={{pid}}&field=registers&sdate={{sdate}}&edate={{edate}}&type={{type}}">注册数</a></th>
                </tr></thead>
                <tbody>
                    <!-- loop result -->
                    <tr>
                        <td><span class="blue" >{{result' value.date}}</span></td>
                        <td>{{result' value.impressions}}</td>
                        <td>{{result' value.impressions_rate}}%</td>
                        <td>{{result' value.clicks}}</td>
                        <td>{{result' value.clicks_rate}}%</td>
                        <td>{{result' value.finish_downloads}}</td>
                        <td>{{result' value.installed}}</td>
                        <td>{{result' value.startups}}</td>
                        <td>{{result' value.registers}}</td>
                    </tr> 
                    <!-- endloop -->
                </tbody>
                <tr>
                    <td><span class="blue" >合计</span></td>
                    <td>{{total.impressions}}</td>
                    <td>{{total.impressions_rate}}%</td>
                    <td>{{total.clicks}}</td>
                    <td>{{total.clicks_rate}}%</td>
                    <td>{{total.finish_downloads}}</td>
                    <td>{{total.installed}}</td>
                    <td>{{total.startups}}</td>
                    <td>{{total.registers}}</td>
                </tr> 
            </table>
        </div>
    </div>
    <div id="container"></div>
</div>
</body>
</html>
<script type="text/javascript">
$(function () {
    $("#showType").click(function(){
        var type = $(this).attr('name');
        if (type == "Day") {
            url = "{{siteUrl}}Statis/getOneByAdId?aid={{aid}}&pid={{pid}}&field=impressions&sdate={{sdate}}&edate={{edate}}";
        }
        if (type == "Hour") {
           url = "{{siteUrl}}Statis/getOneByAdId?aid={{aid}}&pid={{pid}}&field=impressions&sdate={{sdate}}&edate={{edate}}"; 
        }
        window.location.href=url;
    });
    $('#container').highcharts({
        chart: {
            type: 'line',
            marginRight: 25,
            marginBottom: 50
        },
        title: {
            text:"{{title}}",
            x: -20 //center
        },
        xAxis: {
            categories: JSON.parse('{{xlas}}')
        },
        yAxis: {
            title: {
                text: ''
            },
            plotLines: [{
                value: 0,
                width: 1,
                color: '#808080'
            }]
        },
        legend: {
            layout: 'vertical',
            verticalAlign: 'bottom',
            borderWidth: 0
        },
        series: [{
            name: '{{field}}',
            data: JSON.parse('{{chartValue}}')
        }]
    });
});
</script>
