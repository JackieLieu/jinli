{{pagerCss}}
<div class="colMain">
    <div class="title"><p class="fl">自定义广告类型</p></div>
    <div class="main">
        <div class="toolbar">
            <div class="search fl">
                
            </div>
            <a href="edit" class="fr add">新增</a>
        </div>
        <div class="content">
            <div class="grid">
                <table>
                    <thead>
                        <tr>
                            <th>广告类型ID</th>
                            <th>名字</th>
                            <th>支持广告类型</th>
                            <th>父类型</th>
                            <th>操作</th>
                        </tr>
                    </thead>
                    <tbody>
                        <!-- loop result -->
                        <tr>
                            <td><span class="blue">{{result' value.pos_key}}</span></td>
                            <td>{{result' value.pos_name}}</td>
                            <td><?php echo $data['parentArr'][$v1['id']];?></td>
                            <td>
                                <!-- if {{result' value.ad_type}} == 0 -->
                                    抢占式
                                    <!-- if {{result' value.ad_subtype}} == 0 -->- 半屏<!-- else -->- 全屏<!-- endif -->
                                <!-- elseif {{result' value.ad_type}} == 2 -->
                                    PUSH
                                    <!-- if {{result' value.ad_subtype}} == 0 -->- 消息<!-- endif -->
                                    <!-- if {{result' value.ad_subtype}} == 1 -->- APP<!-- endif -->
                                <!-- else -->
                                    嵌入式
                                    <!-- if {{result' value.ad_subtype}} == 0 -->- Banner横幅图片广告<!-- endif -->
                                    <!-- if {{result' value.ad_subtype}} == 1 -->- Banner横幅文字广告<!-- endif -->
                                    <!-- if {{result' value.ad_subtype}} == 2 -->- 插页广告<!-- endif -->
                                    <!-- if {{result' value.ad_subtype}} == 3 -->- 浮窗广告<!-- endif -->
                                <!-- endif -->
                                
                            </td>
                            <td>
                                <a href="edit?id={{result' value.id}}" class="edit" title="编辑"></a>
                                |
                                <a href="delete?id={{result' value.id}}" class="del" title="删除"></a>
                            </td>
                        </tr> 
                        <!-- endloop -->
                    </tbody>
                </table>
            </div>
            <div class="pagination">{{pager.pages}}</div>
        </div>
    </div>
</div>