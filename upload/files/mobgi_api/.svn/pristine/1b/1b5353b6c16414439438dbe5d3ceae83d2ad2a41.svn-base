#chapter_list ul { margin:2px; padding-left:0px}
#chapter_list li { float:left; width:165px; height:20px; font-size: 12px; overflow:hidden; font-family: Tahoma; padding:0 0 0 3; margin:2px; line-height:18px; text-align:left;border: 1px outset #FFFFFF; height:20px}
#chapter_list div{ float: left;}

#collection_image li{float:left; }
#collection_image li div{width:130px; height:130px;border: #ededed 1px solid; margin-bottom: 15px; margin-right:10px; background: #f6f6f6; text-align: center}
#collection_image li .edge{DISPLAY: inline-block;	VERTICAL-ALIGN: middle;	WIDTH: 0px;	HEIGHT: 100%}
#collection_image li .container {	DISPLAY: inline-block;	VERTICAL-ALIGN: middle;	WIDTH: 100%;	TEXT-ALIGN: center}

