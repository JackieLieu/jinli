#segment_list li { float:left; width:175px; height:22px; font-size: 12px; font-family: Tahoma; padding:0 0 0 3; margin:4px; line-height:18px; text-align:left;border: 1px outset #FFFFFF; height:20px; white-space:nowrap}
#segment_list div{ float: left;}
ul.actions {
  float:left;
  padding: 2px;
  margin:0px;
  
} 
ul.actions li {
	margin:0px
	margin-top: 4px;
	margin-bottom:2px;
	background:#fff;
	padding:2px;
	padding-top:6px;
	padding-left:5px;
	font-size: 12px;
	color: #000000;
	background-color: #E6E6CA;
	width:110px;
	cursor: hand;
	border-top: 1px solid #FFFFFF;
	border-right: 1px solid #ACA899;
	border-bottom: 1px solid #ACA899;
	border-left: 1px solid #FFFFFF;
	text-align:center;
	list-style-type: none;
	
}
ul.actions li A:hover {COLOR: #18486b;TEXT-DECORATION: none; white-space: nowrap}
ul.actions li A {white-space: nowrap}
