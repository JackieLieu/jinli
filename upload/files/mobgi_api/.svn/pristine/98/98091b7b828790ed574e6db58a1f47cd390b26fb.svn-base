Doo::loadController("AppDooController");
class IndexController extends AppDooController {
    public function index(){
        $this->myrender("main/index"); 
    }
    public function top(){
        $this->myrender("main/top",$this->data); 
    }
    public function menu(){
        $this->myrender("main/menu",$this->data); 
    }
    public function main(){
        
        $this->myrender("main/main",$this->data); 
    }
    public function fc(){
        $this->myrender("main/fc",$this->data); 
    }
}

